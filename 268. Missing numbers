//Better solution:
class Solution {
    public int missingNumber(int[] nums) {
        int n = nums.length;
        int m;
        int[] hash = new int[n+1];
        for(int i = 0; i<n;i++)
        {
            hash[nums[i]] = 1;
        }
        for(int i =1;i<=n;i++){
            if(hash[i]==0)
            {
                // m = hash[i];
                return i;
            }
        }
        return -1;
    }
}

//Optimal solution:  TC- O(N) SC -O(1)
class Solution {
    public int missingNumber(int[] nums) {
        int n =nums.length;
        int sum = n*(n+1)/2;
        int s2 = 0;
        for(int i=0;i<nums.length;i++)
        {
            s2 = s2+ nums[i];
        }
        int result = sum - s2;
        return result;
    }
}
